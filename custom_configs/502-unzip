for i in PATCH{1..10}; do
   unset $i
done
PATCH1=""

( cat << "xEOFx"
   make -f unix/Makefile generic
   make -j1 prefix=/usr MANDIR=/usr/share/man/man1 \
    -f unix/Makefile install

xEOFx
) > tmp
